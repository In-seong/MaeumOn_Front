{
  "name": "maeumon-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:user": "vite --config vite.user.config.ts",
    "dev:agent": "vite --config vite.agent.config.ts",
    "dev:admin": "vite --config vite.admin.config.ts",
    "build:user": "vue-tsc -b && vite build --config vite.user.config.ts",
    "build:staginguser": "vue-tsc -b && vite build --config vite.staginguser.config.ts",
    "build:agent": "vue-tsc -b && vite build --config vite.agent.config.ts",
    "build:admin": "vue-tsc -b && vite build --config vite.admin.config.ts",
    "build:all": "vue-tsc -b && vite build --config vite.user.config.ts && vite build --config vite.agent.config.ts && vite build --config vite.admin.config.ts",
    "preview:user": "vite preview --config vite.user.config.ts",
    "preview:agent": "vite preview --config vite.agent.config.ts",
    "preview:admin": "vite preview --config vite.admin.config.ts",
    "type-check": "vue-tsc -b"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
}
